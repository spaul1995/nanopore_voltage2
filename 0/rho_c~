FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Temp;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 1 0 0 1 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }

    outlet
    {
        type            zeroGradient;
    }

    front
    {
        type            wedge;
    }

    back
    {
        type            wedge;
    }

    SiNxwalls
    {
        type            zeroGradient;
    }

    topfluid
    {
        type            zeroGradient;
    }
}

